@{
}
@using Animal_Status
@model Animal_Status.VaccinationViewModelIndex

@{
    ViewData["Title"] = "Добавление прививки";
}

<h2>@ViewData["Title"]</h2>

<form asp-action="Create" method="post" enctype="multipart/form-data">
    

    <div class="form-group">
        <label >Прививка</label>
        <select  class="form-control">
            <option value="">Выберите прививку</option>
            @foreach (VaccinationViewModel vaccination in Model.vaccinationViewModels)
            {
                <option value="@vaccination.VaccinationId">@vaccination.Name</option>
            }
        </select>
        <span  class="text-danger"></span>
    </div>

    

    <button type="submit" class="btn btn-primary">Добавить</button>
</form>
